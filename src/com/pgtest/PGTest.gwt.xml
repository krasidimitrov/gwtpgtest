<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="PGTest">

  <!-- Inherit the core Web Toolkit stuff.                  -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.googlecode.gwtphonegap.PhoneGap"/>
  <inherits name="com.googlecode.mgwt.MGWT"/>

  <!-- Specify the app entry point class.                   -->
  <entry-point class='com.pgtest.client.PGTest'/>


  <!-- Specify the app servlets.                   -->
  <servlet path='/PGTestService' class='com.pgtest.server.PGTestServiceImpl'/>


  <set-property name='user.agent' value='safari'/>

  <set-property name="mgwt.os" value="android, android_tablet, desktop" />

</module>